{
    "name": "mobx-devtools",
    "version": "0.9.4",
    "description": "Dev-tools for MobX and React",
    "main": "index.js",
    "typings": "lib/react/index",
    "repository": {
        "type": "git",
        "url": "https://github.com/andykog/mobx-devtools.git"
    },
    "scripts": {
        "publish-all": "yarn ; npm run build:react-mini-panel && node scripts/bump-version-push-publish && npm run build:chrome && npm run build:firefox && npm run publish:chrome && npm run publish:firefox",
        "build": "npm run build:react-mini-panel ; npm run build:chrome ; npm run build:firefox",
        "start:todomvc": "npm run submodules ; npm run start:todomvc-now",
        "start:todomvc-now": "cross-env NODE_ENV=development webpack-dev-server --config playground/webpack.todomvc.config.js --hot --inline",
        "start:todomvc-plain": "cross-env NODE_ENV=development cross-env PLAIN_DEVTOOL=true webpack-dev-server --config playground/webpack.todomvc.config.js --hot --inline",
        "start:bookshop": "npm run submodules ; cd playground/mobx-state-tree/examples/bookshop ; yarn ; cd - ; npm run start:bookshop-now",
        "start:bookshop-now": "cross-env NODE_ENV=development webpack-dev-server --config playground/webpack.bookshop.config.js --hot --inline",
        "start:bookshop-plain": "cross-env NODE_ENV=development cross-env PLAIN_DEVTOOL=true webpack-dev-server --config playground/webpack.bookshop.config.js --hot --inline",
        "submodules": "git submodule update --init ; git submodule foreach yarn",
        "build:react-mini-panel": "cross-env NODE_ENV=production node scripts/react-mini-panel/build",
        "start:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome node scripts/webextension/dev-server.js",
        "start:chrome-debug-connection": "cross-env DEBUG_CONNECTION=true cross-env NODE_ENV=development TARGET_BROWSER=chrome node scripts/webextension/dev-server.js",
        "build:chrome": "cross-env NODE_ENV=production TARGET_BROWSER=chrome node scripts/webextension/build.js",
        "publish:chrome": "cross-env NODE_ENV=production TARGET_BROWSER=chrome node scripts/webextension/publish.js",
        "start:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox node scripts/webextension/dev-server.js",
        "build:firefox": "cross-env NODE_ENV=production TARGET_BROWSER=firefox node scripts/webextension/build.js",
        "publish:firefox": "cross-env NODE_ENV=production TARGET_BROWSER=firefox node scripts/webextension/publish.js",
        "build:mst": "cross-env NODE_ENV=production TARGET_BROWSER=firefox node scripts/mobx-state-tree/build.js",
        "test": "mocha 'src/**/*-test.js' 'shells/**/*-test.js' --compilers js:babel-core/register",
        "lint": "eslint '**/*.js' '**/*.jsx'",
        "lint-fix": "eslint --fix '**/*.js' '**/*.jsx'",
        "prettier": "prettier --write --print-width 100 --tab-width 2 --single-quote \"src/**/*.js\" \"shells/**/*.js\" \"src/**/*.jsx\" \"shells/**/*.jsx\"",
        "precommit": "lint-staged"
    },
    "author": "Michel Weststrate",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mobxjs/mobx/issues"
    },
    "homepage": "https://mobxjs.github.io/mobx",
    "peerDependencies": {
        "mobx": "^2.2.0 || ^3.0.0",
        "mobx-react": "^3.3.0 || ^4.0.0"
    },
    "devDependencies": {
        "aphrodite": "^1.2.4",
        "archiver": "^2.0.3",
        "babel": "^6.23.0",
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "cross-env": "^5.0.1",
        "css-loader": "^0.28.7",
        "del": "^3.0.0",
        "es6-object-assign": "^1.1.0",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-loader": "^1.8.0",
        "eslint-plugin-import": "^2.3.0",
        "eslint-plugin-jsx-a11y": "^5.0.3",
        "eslint-plugin-react": "^7.1.0",
        "file-loader": "^0.11.2",
        "hack-font": "^2.0.20",
        "html-webpack-plugin": "^2.30.1",
        "jsdom": "^11.0.0",
        "jsonwebtoken": "^8.0.1",
        "mobx": "^3.3.0",
        "mobx-react": "^4.3.2",
        "prettier": "^1.6.1",
        "prompt": "^1.0.0",
        "prop-types": "^15.5.10",
        "react": "16",
        "react-addons-create-fragment": "^15.6.2",
        "react-addons-test-utils": "^15.6.0",
        "react-dom": "16",
        "react-test-renderer": "16",
        "react-tiny-virtual-list": "^2.1.2",
        "react-virtualized": "^9.10.1",
        "request": "^2.82.0",
        "shallowequal": "^1.0.2",
        "sinon": "^3.3.0",
        "style-loader": "^0.19.0",
        "ts-loader": "^2.3.7",
        "typescript": "^2.5.2",
        "unexpected": "^10.29.0",
        "unexpected-react": "^4.1.0",
        "unexpected-sinon": "^10.7.1",
        "url-loader": "^0.5.9",
        "webpack": "^3.6.0",
        "webpack-dev-server": "^2.8.2",
        "write-file-webpack-plugin": "^4.1.0"
    },
    "keywords": [
        "mobx",
        "mobservable",
        "react-component",
        "react",
        "reactjs",
        "reactive",
        "devtools"
    ],
    "dependencies": {}
}
